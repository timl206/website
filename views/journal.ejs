<%- include("partials/header"); -%>

<div class="container-fluid">
  <%- include("partials/navigation"); -%>
</div>

<div class="container-fluid view">
  <img src="/img/icons/journal-original.png" alt="..." class="content-icon">
  <h3 class="title-style"> Journal </h3>
  <p class="link-style text">
    A collection of my thoughts.
    <br/><br/>
    I started registering my thoughts on the Ethereum network via Peepeth since end of March '20, you can visit my account by
    <a href="https://peepeth.com/timothylie" target="_blank">
      clicking here.
    </a>
  </p>
  <br/><br/>

  <!-- Journal Posts -->
    <% posts.forEach(function(post){ %>
      <p class="text"> "<%=post.content%>" </p>
      <p class="date"> <%=post.date.toDateString()%> </p>
      <br/>
    <% }) %>

  <p class="link-style"> Fin </p>
</div>

<%- include("partials/footer"); -%>
